.list {
    display: flex;
    /* background: red; */
    position: relative;
}

.list_icon {
    width: 80rpx;
    height: 60rpx;
    display: flex;
    align-items: center;
}

.list_img {
    height: 42rpx;
    width: 42rpx;
}

.list_con {
    flex: 1;
    display: flex;
    align-items: center;
}

.list_con_text {
    flex: 1;
}

.list_con_icon {
    width: 80rpx;
    height: 60rpx;
    display: flex;
    align-items: center;
    justify-content:flex-end;
}

.left_icon {
    height: 30rpx;
    width: 15rpx;
}

.list_num {
    background: red;
    width: 50rpx;
    height: 50rpx;
    border-radius: 40rpx;
    color: #fff;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-right: 10rpx;
}
