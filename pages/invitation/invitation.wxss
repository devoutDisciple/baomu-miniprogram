.wrap {
    height: 100vh;
    width: 100vw;
    background: #f7f7f7;
}

.con {
    height: 100vh;
    width: 100vw;
}

.invitate_btn {
    position: fixed;
    bottom: 40rpx;
    left: 40rpx;
    width: calc(100vw - 80rpx);
    height: 100rpx;
    z-index: 111;
    background: #000;
    color: #fff;
    border-radius: 20rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 38rpx;
}

.calendar {
    width: calc(100vw - 40rpx);
    margin-left: 20rpx;
    margin-top: 20rpx;
}